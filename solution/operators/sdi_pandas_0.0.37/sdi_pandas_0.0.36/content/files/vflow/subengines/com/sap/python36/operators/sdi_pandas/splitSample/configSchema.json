{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "http://sap.com/vflow/sdi_pandas.splitSample.configSchema.json",
    "type": "object",
    "properties": {
        "codelanguage": {
            "type": "string"
        },
        "script": {
            "type": "string"
        },
        "debug_mode": {
            "title": "Debug mode",
            "description": "Sending debug level information to log port",
            "type": "boolean"
        },
        "label_col": {
            "title": "Column of Labels",
            "description": "Label to split",
            "type": "string"
        },
        "split": {
            "title": "Split Factor",
            "description": "Split Factor",
            "type": "number"
        },
        "seed": {
            "title": "Seed",
            "description": "Seed for random number generator",
            "type": "integer"
        },
        "to_category": {
            "title": "To Categorgy",
            "description": "Cast <object> data type to categorical.",
            "type": "boolean"
        }
    }
}